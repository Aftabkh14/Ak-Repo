            <field name="name">Delivered</field>
            <field name="legend_done">Delivered</field>
<!--            <field name="fold" eval="True"/>-->
<!--            <field name="is_closed" eval="True"/>-->
        </record>

        <record id="stage_6" model="project.task.type">
