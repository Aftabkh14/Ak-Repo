<odoo>
        <data noupdate="1">
        <record id="stage_0" model="project.task.type">
            <field name="sequence">1</field>
            <field name="name">BackLog</field>
            <field name="legend_blocked">BackLog</field>
        </record>
        <record id="stage_1" model="project.task.type">
            <field name="sequence">2</field>
            <field name="name">To Do</field>
            <field name="legend_blocked">To Do</field>
            <field name="legend_done">To Do</field>
        </record>
        <record id="stage_2" model="project.task.type">
